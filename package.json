{
	"name": "tree-sitter-swift",
	"version": "0.0.1",
	"description": "Swift grammar for tree-sitter",
	"main": "bindings/node",
	"keywords": [
		"parser",
		"lexer",
		"swift"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/robrix/tree-sitter-swift.git"
	},
	"scripts": {
		"test": "tree-sitter test"
	},
	"author": "Rob Rix",
	"license": "MIT",
	"dependencies": {
		"nan": "^2.1.0"
	},
	"devDependencies": {
		"tree-sitter-cli": "0.19.4"
	},
	"tree-sitter": [
		{
			"scope": "source.swift",
			"file-types": ["swift", "swiftinterface"],
			"highlights": ["queries/highlights.scm"]
		}
	]
}
